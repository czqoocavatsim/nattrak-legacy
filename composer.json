{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-debugbar": "^3.6.8",
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "genealabs/laravel-caffeine": "^10.0.1",
        "guzzlehttp/guzzle": "^7.4.5",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7.2",
        "livewire/livewire": "^2.10.6",
        "marvinlabs/laravel-discord-logger": "^1.2.0",
        "opcodesio/log-viewer": "^2.3",
        "predis/predis": "^1.1.10",
        "sentry/sentry-laravel": "*",
        "spatie/laravel-activitylog": "^4.5.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19.0",
        "jn-jairo/laravel-ngrok": "^3.0",
        "laravel/pint": "^0.2.3",
        "laravel/sail": "^1.15.0",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.2.1",
        "phpunit/phpunit": "^9.5.21",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
